declare
    nux number := 0;
BEGIN
    if '' || '1' || 1 || null != '11' then
        nux := nux + 1;
    end if;

    if length('''''') <> 2 then
        nux := nux + 1;
    end if;

    if '' is not null then
        nux := nux + 1;
    end if;

    if trim('  a   ') != 'a'
        or rtrim(' a ') != ' a'
        or ltrim(' a ') != 'a '
        or trim(null) is not null
        or ltrim(null) is not null
        or ltrim(null) is not null
    then
        nux := nux + 1;
    end if;

    if nux = 0 then
        dbms_output.put_line('OK');
    end if;
END;
/
