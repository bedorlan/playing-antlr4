create or replace package pkgtest is
    function sayhello return varchar2;
    procedure main;
end;
/

create or replace package body pkgtest is
    function sayhello return varchar2 is
    begin
        return 1;
    end;

    procedure main is
        nux number;
    begin
        nux := sayhello + pkgtest.sayhello + sayhello() + pkgtest.sayhello();
        if nux = 4 then
            dbms_output.put_line('OK');
        end if;
    end;
end;
/

begin
    pkgtest.main;
end;
/
