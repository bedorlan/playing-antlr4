create or replace package pkgtest is

    cursor cuData is
        select 'piti' from dual;

    procedure main;
end;
/

create or replace package body pkgtest is
    procedure main is
        sbData varchar2(100);
    begin
      open pkgtest.cuData;
      fetch cuData into sbData;
      close cuData;

      dbms_output.put_line(sbData);
      if sbData = 'X' then
        dbms_output.put_line('OK');
      end if;
    end;
end;
/

begin
  pkgtest.main();
end;
/
